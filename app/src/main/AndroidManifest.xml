<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="alnayzak.ict.ibraheem.scincehouse">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeAr" android:screenOrientation="portrait"></activity>
        <activity android:name=".Tour" android:screenOrientation="portrait"></activity>
        <activity android:name=".TourAR" android:screenOrientation="portrait"></activity>
        <activity android:name=".About" android:screenOrientation="portrait"></activity>
        <activity android:name=".AboutAr" android:screenOrientation="portrait"></activity>
        <activity android:name=".Gifts" android:screenOrientation="portrait"></activity>
        <activity android:name=".GiftsAr" android:screenOrientation="portrait"></activity>
        <activity android:name=".Contact" android:screenOrientation="portrait"></activity>
        <activity android:name=".ContactAr" android:screenOrientation="portrait"></activity>
        <activity android:name=".NewsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".NewsARActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".GiftDetailsActivity" android:screenOrientation="portrait"></activity>
        <activity android:name=".GiftDetailsARActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>